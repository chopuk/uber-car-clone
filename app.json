{
  "expo": {
    "name": "uber-car-clone",
    "slug": "uber-car-clone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bigchopper.ubercarclone",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyABEMsMaweeuhEW9zx2tOMthHqyNu7Lbp0"
        },
        "permissions": ["ACCESS_COARSE_PERMISSION","ACCESS_FINE_LOCATION","FOREGROUND_SERVICE"]
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "6ae521ac-0787-430a-aa23-23012231a470"
      }
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ]
  }
}